  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "@types/node": "^22.5.5",
    "typescript": "^5.7.0",
    "vite": "^7.1.5",
    "vue-tsc": "^2.2.0"
  }
}

